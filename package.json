{
  "name": "@tasks-timeline/components",
  "description": "A comprehensive React component library for task management and timeline visualization",
  "private": false,
  "version": "0.1.3",
  "type": "module",
  "license": "MIT",
  "author": "Leonezz <zhwuenqa@outlook.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Leonezz/tasks-timeline-components.git"
  },
  "bugs": {
    "url": "https://github.com/Leonezz/tasks-timeline-components/issues"
  },
  "homepage": "https://github.com/Leonezz/tasks-timeline-components#readme",
  "keywords": [
    "react",
    "components",
    "task-management",
    "timeline",
    "todo",
    "ui",
    "typescript"
  ],
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.js",
      "require": "./dist/components/index.umd.cjs"
    },
    "./hooks": {
      "types": "./dist/hooks/index.d.ts",
      "import": "./dist/hooks/index.js",
      "require": "./dist/hooks/index.umd.cjs"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js",
      "require": "./dist/types/index.umd.cjs"
    },
    "./index.css": "./dist/index.css"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "prepare": "vite build && tsc -p tsconfig.build.json",
    "build": "tsc -p tsconfig.build.json && vite build",
    "build:lib": "vite build && tsc -p tsconfig.build.json",
    "dev:lib": "tsc -p tsconfig.build.json --watch",
    "preview": "vite preview",
    "lint": "eslint src",
    "type-check": "tsc --noEmit",
    "test": "vitest --project=unit",
    "test:watch": "vitest --project=unit --watch",
    "storybook": "storybook dev -p 6006",
    "test-storybook": "vitest --project=storybook",
    "build-storybook": "storybook build",
    "dev:example": "cd examples/app && pnpm dev",
    "build:example": "cd examples/app && pnpm build",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\""
  },
  "workspaces": [
    "examples/app"
  ],
  "dependencies": {
    "@google/genai": "^1.34.0",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-portal": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "expr-eval": "^2.0.2",
    "framer-motion": "12.23.26",
    "lucide-react": "^0.330.0",
    "luxon": "3.4.4",
    "next-themes": "^0.4.6",
    "react-error-boundary": "^6.0.3",
    "react-shadow": "^20.6.0",
    "rrule": "2.8.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0"
  },
  "peerDependencies": {
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "openai": ">=4.0.0",
    "@anthropic-ai/sdk": ">=0.30.0"
  },
  "peerDependenciesMeta": {
    "openai": { "optional": true },
    "@anthropic-ai/sdk": { "optional": true }
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.3",
    "@eslint/js": "^9.39.1",
    "@faker-js/faker": "^9.3.0",
    "@storybook/addon-a11y": "^10.1.11",
    "@storybook/addon-docs": "^10.1.11",
    "@storybook/addon-onboarding": "^10.1.11",
    "@storybook/addon-vitest": "^10.1.11",
    "@storybook/react-vite": "^10.1.11",
    "@tailwindcss/vite": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/luxon": "^3.7.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/browser": "^4.0.18",
    "@vitest/browser-playwright": "^4.0.18",
    "assert": "^2.1.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-storybook": "^10.1.11",
    "globals": "^16.5.0",
    "playwright": "^1.57.0",
    "prettier": "^3.7.4",
    "storybook": "^10.1.11",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vitest": "^4.0.16"
  },
  "packageManager": "pnpm@10.27.0+sha512.72d699da16b1179c14ba9e64dc71c9a40988cbdc65c264cb0e489db7de917f20dcf4d64d8723625f2969ba52d4b7e2a1170682d9ac2a5dcaeaab732b7e16f04a"
}
